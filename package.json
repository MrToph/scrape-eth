{
	"name": "scrape-eth",
	"version": "1.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "tsc",
		"start": "npm run build && node dist/cli.js",
		"pretest": "npm run build",
		"test": "xo && ava"
	},
	"files": [
		"dist/cli.js"
	],
	"dependencies": {
		"ink": "^3.0.8",
		"meow": "^9.0.0",
		"react": "^17.0.2"
	},
	"devDependencies": {
		"@ava/typescript": "^1.1.1",
		"@sindresorhus/tsconfig": "^1.0.2",
		"@types/react": "^17.0.6",
		"ava": "^3.15.0",
		"chalk": "^4.1.1",
		"eslint-config-xo-react": "^0.25.0",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"ink-testing-library": "^2.1.0",
		"typescript": "^4.2.4",
		"xo": "^0.40.1"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"tsx"
			],
			"rewritePaths": {
				"source/": "dist/"
			}
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	}
}
